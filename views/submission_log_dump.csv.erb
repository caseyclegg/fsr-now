<%= CSV.generate_line(['id', 'name', 'jobRole', 'company', 'emailAddress', 'busPhone', 'country', 'usStates', 'caTerritories', 'ukBoroughs', 'postal1', 'description1', 'status', 'created_at']) %>
<% @submissions.each do |submission| %>
  <%= CSV.generate_line([submission.id, submission.name, URI.unescape(submission.jobRole || ''), submission.company_name, submission.email, submission.phone, URI.unescape(submission.country || ''), URI.unescape(submission.usStates || ''), URI.unescape(submission.caTerritories || ''), URI.unescape(submission.ukBoroughs || ''), URI.unescape(submission.postal1 || ''), submission.description, submission.created_at, submission.status], options = {:row_sep => ''}) %>
<% end %>