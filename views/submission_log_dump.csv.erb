<%= CSV.generate_line(['id', 'firstName', 'lastName', 'jobRole', 'company', 'emailAddress', 'busPhone', 'country', 'usStates', 'caTerritories', 'ukBoroughs', 'postal1', 'description1', 'status', 'created_at']) %>
<% @submissions.each do |submission| %>
