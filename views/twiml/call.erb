<?xml version="1.0" encoding="UTF-8"?>
<Response>
	<Gather action="<%= @submission.twilio_gather_url %>" method="GET" timeout="15" numDigits="1">
		<Say voice='alice'>This is a Twilio full services request from <%=  @submission.name %><%= ' at '+@submission.company unless @submission.company.is_blank? %>, please press one to call the lead now.</Say>
  </Gather>
  <Redirect method="GET">
    <%= @submission.twilio_gather_url %>&amp;Digits=TIMEOUT
  </Redirect>
</Response>